{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "yeet": "ng serve --open --watch --poll=2000",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json --poll=2000",
    "serve:docker": "ng serve --configuration=docker --poll=2000",
    "serve:docker-alt": "ng run frontend:serve:docker",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:frontend": "node dist/frontend/server/server.mjs",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
